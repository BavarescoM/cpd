{{#if success_msg}}
        <div class="alert alert-success">{{success_msg}}</div>
    
{{/if}}

{{#if error_msg}}
    <div class="alert alert-danger">{{error_msg}}</div>
{{/if}}

{{#if error_form}}
    {{#each error_form}}
      <div class="alert alert-danger">{{message}}</div>
    {{else}}    
    {{/each}}
{{/if}}